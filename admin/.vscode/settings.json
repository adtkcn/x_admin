{
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "cSpell.words": [
        "brotli",
        "btns",
        "daterange",
        "datetimerange",
        "echarts",
        "execa",
        "highlightjs",
        "logicflow",
        "nprogress",
        "pinia",
        "preinstall",
        "rushstack",
        "tailwindcss",
        "unplugin",
        "vform",
        "vuedraggable",
        "vueuse",
        "wangeditor"
    ]
}
